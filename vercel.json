{
    "version": 2,
    "builds": [
      {
        "src": "pubspec.yaml",
        "use": "@vercel/flutter",
        "config": {
          "entrypoint": "lib/main_web.dart"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  