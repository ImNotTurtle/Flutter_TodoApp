{
    "version": 2,
    "builds": [
      {
        "src": "/**",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "sh vercel-build.sh",
          "outputDirectory": "build/web"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  